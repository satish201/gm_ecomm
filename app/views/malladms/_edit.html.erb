<div style="height: 894px; overflow: scroll;">
<%= form_for(@malladmin, :validate => true, :url => malladm_path + ".js", :html => {:multipart => true, :target => 'frame'} ) do |f| %>
   <% if @malladmin.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@malladmin.errors.count, "error") %> prohibited this malladmin from being saved:</h2>

      <ul>
      <% @malladmin.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  
  <div>
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </div>
  <div>
    <%= f.label :email %><br />
